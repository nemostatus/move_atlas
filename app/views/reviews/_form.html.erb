<%=render "shared/nav"%>
<%= form_for([@vehicle, @review]) do |vehicle_review| %>
    <%=vehicle_review.label :customer_experience_rating%>
    <%=vehicle_review.number_field :customer_experience_rating%>
    <%= vehicle_review.hidden_field :vehicle_id, value: @vehicle.id %>
    <%=vehicle_review.submit%>
    <% end %>