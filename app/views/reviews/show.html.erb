<%=render "shared/nav"%>
<% @vehicle.reviews.each do |review| %>
    <%= review.customer_experience_rating %>
    <%= review.customer_service_rating %>
    <%= review.customer_service_rating + review.customer_experience_rating%>

    <b>By:</b> <%= link_to review.user.username, user_path(review.user) %><br>
    <b>Posted:</b> <%= review.created_at %><br>
    <br>
    <br>
   
  <% end %>