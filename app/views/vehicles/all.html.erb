<%=render "shared/nav"%>
<u><h1> Your Vehicles </h1></u>
<h2> Clean Vehicles </h2>
<% @clean_vehicles.each do |vehicle| %>
<br>
  Vehicle Plate: <%= link_to vehicle.plate_number, vehicle_path(vehicle)%>
  <br>
  Rented From:</b> <%= vehicle.company_name %>
  <br>
  On: <%= vehicle.pick_up_date %>
  <br>
  
  <%end%>
 
  
 <h2> Infested Vehicles </h2>
    <% @infested_vehicles.each do |vehicle| %>
         <br>
      Vehicle: <%=vehicle.plate_number%>
      <br>
        From:</b> <%= vehicle.company_name %>
         <br>
      Infested with: </b> <%= vehicle.bug_type %>
        <br>
        On: <%= vehicle.pick_up_date %>
        <br>
        <%= link_to "Edit", edit_vehicle_path(vehicle)%>
        <%= button_to "Delete", vehicle_path(vehicle), method: :delete %> 
     <% end %>
     <br>

